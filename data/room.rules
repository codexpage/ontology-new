
@include <RDFS>.
@include <OWL>.
@prefix ywx: <http://www.semanticweb.org/ywx/ontologies/2016/6/tju#>


#温度
[rule1:(ywx:temperaturesensor1 ywx:hasvalue ?y),greaterThan(?y,30) -> (ywx:openairconditioner ywx:associate ywx:newI)] 
[rule2:(ywx:temperaturesensor1 ywx:hasvalue ?y),lessThan(?y,30) -> (ywx:closeairconditioner ywx:associate ywx:newI)]

#luminance
[rulel1:(ywx:luminancesensor1 ywx:hasvalue ?y),greaterThan(?y,300) -> (ywx:openbulball ywx:associate ywx:newI)]
[rulel2:(ywx:luminancesensor1 ywx:hasvalue ?y),lessThan(?y,300) -> (ywx:openbulball ywx:associate ywx:newI)]

#湿度
[rule3:(?x ywx:humidity ?y),greaterThan(?y,65) -> (ywx:closehumidifierI ywx:associate ywx:newI)]
[rule4:(?x ywx:humidity ?y),lessThan(?y,40) -> (ywx:openhumidifierI ywx:associate ywx:newI)]


